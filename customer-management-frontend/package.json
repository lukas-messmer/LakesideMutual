{
  "name": "customer-management-frontend",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "flow-bin": "^0.244.0",
    "react-scripts": "5.0.1"
  },
  "dependencies": {
    "documentation": "^14.0.3",
    "moment": "^2.30.1",
    "normalizr": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "react-stomp": "^5.1.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "whatwg-fetch": "^3.6.20"
  },
  "scripts": {
    "prestart": "react-env",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "doc": "documentation build src/** -f html -o docs",
    "flow": "flow",
    "openapi-generate": "npx openapi-generator-cli generate -g javascript -i http://localhost:8080/v3/api-docs -o ./src/openapi/customer-self-service-backend --global-property apis,models && npx openapi-generator-cli generate -g javascript -i http://localhost:8100/v3/api-docs -o ./src/openapi/customer-management-backend --global-property apis,models"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
